		function settotalprize(){
			currency   = $('#currency').val();
			finalprize = '0';
			addonfeatured  = $('input[name="jobpost[featured]"]');
			addonrecruiter = $('input[name="jobpost[addon_recruiter]"]');
			addonnda       = $('input[name="jobpost[addon_nda]"]');
			addonurgent    = $('input[name="jobpost[addon_urgent]"]');
			addonprivate   = $('input[name="jobpost[addon_private]"]');
			fcost   = 500;
			rcost   = 2000;
			ncost   = 1000;
			ucost   = 300;
			pcost   = 300;
			if(currency == 'USD'){
				fcost   = 10;
				rcost   = 30;
				ncost   = 15;
				ucost   = 5;
				pcost   = 5;				
			}	
			$('#isfeatured').html('<b>'+fcost + ' ' + currency+'</b>');
			$('#isrecruiter').html('<b>'+rcost + ' ' + currency+'</b>');
			$('#isnda').html('<b>'+ncost + ' ' + currency+'</b>');
			$('#isurgent').html('<b>'+ucost + ' ' + currency+'</b>');
			$('#isprivate').html('<b>'+pcost + ' ' + currency+'</b>');

			switch(freeaddon){
				case 'recruiter':
					rcost = 0;
					$('#isrecruiter').html('<b>FREE OFFER</b>');	
					$('input[name="jobpost[addon_recruiter]"]').attr('checked',true);	
				break;
				case 'nda':
					ncost = "0";
					$('#isnda').html('<b>FREE OFFER</b>');
					$('input[name="jobpost[addon_nda]"]').attr('checked',true);
				break;
				case 'urgent':
					ucost = "Free";
					$('#isurgent').html('<b>FREE OFFER</b>');
					$('input[name="jobpost[addon_urgent]"]').attr('checked',true);
				break;
				case 'private':
					pcost = "Free";
					$('#isprivate').html('<b>FREE OFFER</b>');
					$('input[name="jobpost[addon_private]"]').attr('checked',true);
				break;
				case 'featured':
					fcost = "Free";
					$('#isfeatured').html('<b>FREE OFFER</b>');
					$('input[name="jobpost[featured]"]').attr('checked',true);
				break;				
			}			

			if(addonfeatured.is(":checked")){
			  finalprize =  parseInt(finalprize) + parseInt(fcost);
			}
			if(addonrecruiter.is(":checked")){
			  finalprize = parseInt(finalprize) + parseInt(rcost);
			}
			if(addonnda.is(":checked")){
			  finalprize = parseInt(finalprize) + parseInt(ncost);
			}
			if(addonurgent.is(":checked")){
			  finalprize = parseInt(finalprize) + parseInt(ucost);
			}
			if(addonprivate.is(":checked")){
			  finalprize = parseInt(finalprize) + parseInt(pcost);
			}
			if(finalprize){
				$('#total-currency').html(currency);
				$('#total').html(finalprize);
			}
			$('#total-amount').val(finalprize);
			$('#tobepaid-currency').html(currency);			
		}

