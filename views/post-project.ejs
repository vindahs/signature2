		function checktitle(str){
			for(var i = 0, len = str.length, count = 0, ch; i < len; ++i){
				ch = str.charAt(i);
				if(ch >= 'A' && ch <= 'Z') ++count;
				text = str.length;
			}
			percentage = (count/text) * 100;
			percentage = Math.round(percentage);
			return percentage;
		}


