<script type="text/javascript">
function socketClose(){
    if(window.sess_uid != "" || window.sess_uid != undefined){
        socket.close();
    }
}

function socketOpen(){
    if(window.sess_uid != "" || window.sess_uid != undefined){
        socket.open();
        //console.log("window focus socket for userId " + window.sess_uid);
        if(window.page == "pg_workstream"){
            var lastinteractionid = $( ".interaction_box" ).last().attr('data-wsinid');
            var action_url= window.base_url+"/ajax/getlatestinteraction";
            $.ajax({
                type: 'POST',
                url:action_url,
                data:{wsinid:lastinteractionid},
                datatype: "html",
                success: function(response){
                    $('#replybox').before(response.html);
                }
            });
        }
        for(var i = 0;i < popups.length;i++){
            var chatboxdetails = popups[i];
            var details = chatboxdetails.split('_');
            var workstreamid = details[1];
            var senderid = $('#'+chatboxdetails).attr('data-selfid');
            var receiverid = $('#'+chatboxdetails).attr('data-otherid');
            $('#messages_chatbox_'+details[1]).html('');
            getworkstream_interations(workstreamid,senderid,receiverid);
            console.log('workstreamid',workstreamid,senderid,"selfid");
        }
        
        if(window.page == "job_page" && window.sess_uid == window.job_uid){
            $('#get_allproposals').click();
        }
    }
}


var visibility = (function(){
    var stateKey, 
    eventKey, 
    keys = {
        hidden: "visibilitychange",
        webkitHidden: "webkitvisibilitychange",
        mozHidden: "mozvisibilitychange",
        msHidden: "msvisibilitychange"
    };
    for(stateKey in keys){
        if(stateKey in document){
            eventKey = keys[stateKey];
            break;
        }
    }
    return function(c){
        if (c) document.addEventListener(eventKey, c);
        return !document[stateKey];
    }
})();


visibility(function(){    
    if(visibility()){
        socketOpen();                                       
    }else{
       socketClose();
    }
});

var notIE = (document.documentMode === undefined),
isChromium = window.chrome;    
if(notIE && !isChromium){
    // checks for Firefox and other  NON IE Chrome versions
    // $(window).on("focusin", function(){ 
    //   socketOpen();
    // }).on("focusout", function(){
    //     socketClose();
    // });
}else{
    // checks for IE and Chromium versions
    if(window.addEventListener){
        // bind focus event
        window.addEventListener("focus", function(event){
           socketOpen();
        }, false);
        // bind blur event
        window.addEventListener("blur", function(event){
            socketClose();
        }, false);
    }else{
        // bind focus event
        window.attachEvent("focus", function(event){
           socketOpen();
        });
        // bind focus event
        window.attachEvent("blur", function(event){
           socketClose();
        });
    }
}
</script>
