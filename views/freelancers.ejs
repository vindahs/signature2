<script>
$(document).ready(function(){
	$('.loader').hide();
	$('select[name="sort"]').on('change',function(){
 		$('#searchbutton').click();
 	});

 	$('#currency').hide();
 	var sessioncurrency=$('#currency').attr('data-currency');
 	$('.filter-sort').select2({
   		 minimumResultsForSearch: -1
	});
 	$( "#customcountry" ).autocomplete({
      source: window.base_url+"/ajax/get_countrylist",
      minLength: 2,
      select: function( event, ui ) {
      	if(ui.item){
      		$('#customcountry').val('');
      		var countrylist=[];
			countrys = $('input[name="countrycode"]');
			countrys.each(function(){
				if($(this).val() != 'All'){
					countrylist.push($(this).val());
				}
			});
			//console.log('list- '+countrylist);
			//console.log('item- '+ui.item.id);
			if(countrylist.indexOf(ui.item.id) >= 0){
				countrys.each(function(){
				if($(this).val() != 'All' && $(this).val() == ui.item.id){
					$(this).prop('checked',true);
				}
				});
			}else{
				html = '<li><span class="display-ib"><div class="squaredFour"><input type="checkbox" checked id="squaredFour-'+ui.item.id+'" class="filter_country" name="countrycode" value="'+ui.item.id+'"><label for="squaredFour-'+ui.item.id+'"></label></div></span><a href="'+window.base_url+'/freelancers?clist='+ui.item.id+'">'+ui.item.value+'</a></li>';
				$('.fil_sec_country ul').append(html);
			}
			$('.filter_country.all').prop('checked',false);
			update_list();

      	}
      	$(this).val(''); return false;

      }
    });

 	$("#customskill").autocomplete({
 		source: window.base_url+"/ajax/get_skilllist",
 		minLength: 2,
 		select:function(event, ui){
	      	if(ui.item){
	      		$('#customskill').val('');
	      		var skilllist = [];
	      		skills = $('input[name="skill"]');
	      		skills.each(function(){
	      			if($(this).val() != 'All'){
	      				skilllist.push($(this).val());
	      			}
	      		});
	      		//console.log('list- '+skilllist);
	      		//console.log('item- '+ui.item.id);
	      		if(skilllist.indexOf(''+ui.item.id+'') >= 0){
	      			skills.each(function(){
	      				if($(this).val() != 'All' && $(this).val() == ui.item.id){
	      					$(this).prop('checked',true);
	      				}
	      			});
	      		}else{
	      			html = '<li><span class="display-ib"><div class="squaredFour"><input type="checkbox" checked id="squaredFour-'+ui.item.id+'" class="filter_skill" name="skill" value="'+ui.item.id+'"><label for="squaredFour-'+ui.item.id+'"></label></div></span><a href="'+ui.item.link+'">'+ui.item.value+'</a></li>';
	      			$('.fil_sec_skill ul').append(html);
	      		}
	      		$('.filter_skill.all').prop('checked',false);
	      		update_list();
	      	}
	    	$(this).val('');
	    	return false;
      	}
    });
 	$(function() {
 	var currency=$('#currency').html();
 	if(sessioncurrency=='USD'){
 		var maxrange='200';
 	}else if(sessioncurrency=='INR'){
 		var maxrange='5000';
 	}
	var minrange = 0;
		 	$('#maximum').val(maxrange);
 	$( "#price-slider-range" ).slider({
      range: true,
      min: 0,
      max: maxrange,
      values: [minrange,maxrange],
      slide: function( event, ui ) {
        $( "#amount" ).html( currency + ui.values[ 0 ] + " - "+currency + ui.values[ 1 ] );
        $('#minimum').val(ui.values[ 0 ]);
        $('#maximum').val(ui.values[ 1 ]);
      },
      change: function( event, ui ) {
        update_list();
      }
    });

    $( "#amount" ).html( currency + $( "#price-slider-range" ).slider( "values", 0 ) +
      " - "+currency + $( "#price-slider-range" ).slider( "values", 1 ) );

  });

 	function update_list(){

 		var countrylist=[];
 		var skilllist = [];
 		var min=$('#minimum').val();
        var max=$('#maximum').val();
        var q = $('#search').val();
        selectedcountry = $('input[name="countrycode"]:checked');
        selectedskill = $('input[name="skill"]:checked');
		var clist = "";
		var slist = "";
		selectedcountry.each(function(){
			if($(this).val() != 'All'){
				countrylist.push($(this).val());
				clist += $(this).val()+ ",";
			}
		})
		if(clist != ''){
			clist = clist.slice(0, - 1);
		}

		if(countrylist.length == 0){
			$('.filter_country').removeAttr('checked');
			$('.filter_country.all').prop('checked',true);
		}

		selectedskill.each(function(){
			if($(this).val() != 'All'){
				skilllist.push($(this).val());
				slist += $(this).val()+ ",";
			}
		})
		if(slist != ''){
			slist = slist.slice(0, - 1);
		}

		if(skilllist.length == 0){
			$('.filter_skill').removeAttr('checked');
			$('.filter_skill.all').prop('checked',true);
		}

		// send ajax request for filter
		//console.log(clist);
		var action_url= window.base_url+"/freelancers";
		action_url = action_url +'?ajax=true&q='+q+'&clist='+clist+'&slist='+slist+'&currency='+sessioncurrency+'&pmin='+min+'&pmax='+max;

					ga('send', 'pageview', action_url);
		
			$('.freelancer-list').hide();
			$('.loader').show();
			$.ajax({
				type: 'GET',
				url:action_url,
				datatype: "html",
				success: function(response){
					$('.loader').hide();
					$('.freelancer-list').html(response);
					$('.freelancer-list').show();
					$("img.lazy").lazyload();
				}
		    });
 	}
	$(document).on('change','input[name="countrycode"]',function(){
		if($(this).val() != 'All'){
			$('.filter_country.all').removeAttr('checked');
		}else{
			$('.filter_country').removeAttr('checked');
			$('.filter_country.all').prop('checked',true);
		}
		update_list();
		return false;
	});
	$(document).on('change','input[name="skill"]',function(){
		if($(this).val() != 'All'){
			//console.log('not all');
			$('.filter_skill.all').removeAttr('checked');
		}else{
			//console.log('all');
			$('.filter_skill').removeAttr('checked');
			$('.filter_skill.all').prop('checked',true);
		}
		update_list();
		return false;
	});

	$('.filtercountrycollapse').on('hidden.bs.collapse', function () {
		$('.countrycollapsedown').show();
		$('.countrycollapseup').hide();
	});

	$('.filtercountrycollapse').on('shown.bs.collapse', function () {
		$('.countrycollapsedown').hide();
		$('.countrycollapseup').show();
	});

	$('.filterskillcollapse').on('hidden.bs.collapse', function () {
		$('.skillcollapsedown').show();
		$('.skillcollapseup').hide();
	});

	$('.filterskillcollapse').on('shown.bs.collapse', function () {
		$('.skillcollapsedown').hide();
		$('.skillcollapseup').show();
	});

 });
</script>
